		"@nextcloud/paths": "^1.1.0",
		"@nextcloud/router": "^1.0.0",
		"@nextcloud/vue": "1.3.0",
		"axios": "^0.19.2",
		"cdav-library": "github:nextcloud/cdav-library",
		"core-js": "^3.6.4",
		"debounce": "^1.2.0",
		"downloadjs": "^1.4.7",
		"ical.js": "^1.3.0",
