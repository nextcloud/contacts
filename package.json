		"uuid": "^3.4.0",
		"vue": "^2.6.11",
		"vue-click-outside": "^1.0.7",
		"vue-clipboard2": "^0.3.1",
		"vue-router": "^3.1.5",
		"vue-virtual-scroller": "^1.0.0-rc.2",
		"vuex": "^3.1.2",
		"vuex-router-sync": "^5.0.0"
	},
	"engines": {
		"node": ">=10.0.0"
	},
